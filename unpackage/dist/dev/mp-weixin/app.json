{
  "pages": [
    "pages/index/index",
    "pages/my/my"
  ],
  "subPackages": [
    {
      "root": "indexpackage",
      "pages": [
        "pages/goodsdetail/goodsdetail"
      ]
    },
    {
      "root": "mypackage",
      "pages": [
        "pages/about/about",
        "pages/about/privacypolicy"
      ]
    }
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "backgroundColor": "#ffffff",
    "easycom": {
      "autoscan": true,
      "custom": {
        "^u--(.*)": "./uni_modules/uview-plus/components/u-$1/u-$1.vue",
        "^up-(.*)": "./uni_modules/uview-plus/components/u-$1/u-$1.vue",
        "^u-([^-].*)": "./uni_modules/uview-plus/components/u-$1/u-$1.vue"
      }
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#e94359",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "商城",
        "iconPath": "static/icons/tab1.png",
        "selectedIconPath": "static/icons/tab1_active.png"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "static/icons/tab2.png",
        "selectedIconPath": "static/icons/tab2_active.png"
      }
    ]
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "indexpackage"
      ]
    },
    "pages/my/my": {
      "packages": [
        "mypackage"
      ]
    }
  },
  "requiredPrivateInfos": [
    "getLocation",
    "chooseLocation"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": ""
    }
  },
  "usingComponents": {}
}