{
  "pages": [
    "pages/index/index",
    "pages/my/my"
  ],
  "subPackages": [
    {
      "root": "packageA",
      "pages": [
        "pages/goodsdetail/goodsdetail"
      ]
    },
    {
      "root": "packageB",
      "pages": [
        "pages/about/about",
        "pages/about/privacypolicy"
      ]
    }
  ],
  "usingComponents": {
    "van-tabbar": "/wxcomponents/vant/tabbar/index",
    "van-tabbar-item": "/wxcomponents/vant/tabbar-item/index",
    "van-icon": "/wxcomponents/vant/icon/index",
    "van-search": "/wxcomponents/vant/search/index",
    "van-button": "/wxcomponents/vant/button/index",
    "van-cell": "/wxcomponents/vant/cell/index",
    "van-tabs": "/wxcomponents/vant/tabs/index",
    "van-tab": "/wxcomponents/vant/tab/index",
    "van-popup": "/wxcomponents/vant/popup/index",
    "van-slider": "/wxcomponents/vant/slider/index"
  },
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "backgroundColor": "#ffffff"
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#e94359",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "商城",
        "iconPath": "static/icons/tab1.png",
        "selectedIconPath": "static/icons/tab1_active.png"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "static/icons/tab2.png",
        "selectedIconPath": "static/icons/tab2_active.png"
      }
    ]
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "packageA"
      ]
    },
    "pages/my/my": {
      "packages": [
        "packageB"
      ]
    }
  },
  "permission": {
    "scope.userLocation": {
      "desc": ""
    }
  }
}