<import-sjs src="./index.sjs" module="computed" />

<view class="{{ computed.rootClass({ classPrefix, name }) }}" style="{{ computed.rootStyle({ customStyle, color, size }) }}" bindtap="onClick">
  <van-info s-if="info !== null || dot" dot="{{ dot }}" info="{{ info }}" custom-class="van-icon__info info-class" />
  <image s-if="computed.isImage(name)" src="{{ name }}" mode="aspectFit" class="van-icon__image" />
</view>
